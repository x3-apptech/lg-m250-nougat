cust_accel@68 {
	compatible = "mediatek,bmi160_acc";
	reg = <0x68>;
	status = "okay";
	i2c_num = <3>;
	i2c_addr = <0x68 0 0 0>;
	direction = <4>;
	power_id = <0xffff>;
	power_vol = <0>;
	firlen = <0>;
	is_batch_supported = <0>;
};
